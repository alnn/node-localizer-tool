{
  "name": "node-localizer-tool",
  "version": "0.0.0",
  "description": "Localizer tool",
  "main": "./build/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "babel src -d build",
    "start": "npm run build && node ./demo/index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ernium/node-localizer-tool.git"
  },
  "keywords": [
    "localize",
    "language",
    "i18n",
    "translate"
  ],
  "author": "Alexander Kazmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ernium/node-localizer-tool/issues"
  },
  "homepage": "https://github.com/ernium/node-localizer-tool#readme",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "co": "^4.6.0",
    "immutable": "^3.8.1",
    "key-mirror": "^1.0.1",
    "mongoose": "^4.6.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "mocha": "^3.0.2",
    "redux-mock-store": "^1.2.0",
    "sinon": "^1.17.5",
    "sinon-mongoose": "^1.3.0",
    "socket.io-client": "^1.4.8"
  }
}
